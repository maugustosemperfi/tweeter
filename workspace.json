{
  "version": 2,
  "cli": {
    "defaultCollection": "@nrwl/angular"
  },
  "generators": {
    "@nrwl/angular:application": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/angular:library": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest"
    },
    "@nrwl/angular:component": {
      "style": "scss"
    }
  },
  "projects": {
    "tweeter-data": {
      "projectType": "library",
      "root": "libs/tweeter-data",
      "sourceRoot": "libs/tweeter-data/src",
      "prefix": "tweeter",
      "targets": {
        "build": {
          "executor": "@nrwl/angular:package",
          "options": {
            "tsConfig": "libs/tweeter-data/tsconfig.lib.json",
            "project": "libs/tweeter-data/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "libs/tweeter-data/tsconfig.lib.prod.json"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/tweeter-data/src/**/*.ts",
              "libs/tweeter-data/src/**/*.html"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/libs/tweeter-data"
          ],
          "options": {
            "jestConfig": "libs/tweeter-data/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "tweeter-ui": {
      "projectType": "library",
      "root": "libs/tweeter-ui",
      "sourceRoot": "libs/tweeter-ui/src",
      "prefix": "tweeter",
      "targets": {
        "build": {
          "executor": "@nrwl/angular:package",
          "options": {
            "tsConfig": "libs/tweeter-ui/tsconfig.lib.json",
            "project": "libs/tweeter-ui/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "libs/tweeter-ui/tsconfig.lib.prod.json"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/tweeter-ui/src/**/*.ts",
              "libs/tweeter-ui/src/**/*.html"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/libs/tweeter-ui"
          ],
          "options": {
            "jestConfig": "libs/tweeter-ui/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    }
  }
}